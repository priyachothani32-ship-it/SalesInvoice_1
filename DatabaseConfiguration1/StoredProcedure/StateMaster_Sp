USE [DatabaseConfiguration1]
GO
/****** Object:  StoredProcedure [dbo].[StateMaster_Sp]    Script Date: 05-09-2025 14:09:21 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
ALTER        proc [dbo].[StateMaster_Sp]
@status nvarchar(max) ='',
@stateName nvarchar(max) =''
AS

BEGIN
	if @status='insert'
		BEGIN
			insert into StateMaster (State_Name)values(@stateName);
		END
	
	else if @status='select'
		Begin
			select State_Id,State_Name as StateName from StateMaster;
		END
END
